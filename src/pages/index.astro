---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/Navbar.astro";
import Slider from "../components/Slider.astro";
import Footer from "../components/Footer.astro";
import Products from "../components/Products.astro";
import CampoGolf from "../components/CampoGolf.astro";
import Nosotros from "../components/Nosotros.astro";
import Amenidades from "../components/Amenidades.astro";
import Contacto from "../components/Contacto.astro";
import Video from "../components/Video.astro";
import Rest1900 from "../components/Rest1900.astro";
import MapCotizador from "../components/MapCotizador.astro";
import i18next, { t, changeLanguage } from "i18next";

changeLanguage("es");
---
<Layout title={t("layout.title")}>
	<main>
		<Navbar/>
		<Slider/>
		<MapCotizador/>
		<Nosotros/>
		<Products/>
		<CampoGolf/>
		<Amenidades/>
		<Video/>
		<Rest1900/>
		<Contacto/>	
		<Footer/>
	</main>
</Layout>

